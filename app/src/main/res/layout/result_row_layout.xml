<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_marginBottom="1dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="40dp"
        android:layout_height="match_parent"
        android:id="@+id/sirialNoRR"
        android:gravity="center"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/nameRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="75dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/dipositRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/millRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/millCostRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:text="Extra"
        android:gravity="center"
        android:id="@+id/extraRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/totalCostRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
    <TextView
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/restMonyRR"
        android:background="#00A7E5"
        android:layout_marginRight="1dp"
        android:padding="2dp"
        android:textSize="@dimen/resultTextSize"
        android:textColor="@color/white"/>
</LinearLayout>